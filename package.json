{
  "name": "environment-switch",
  "description": "Quickly switch project pages between environments",
  "private": true,
  "version": "0.2.2",
  "type": "module",
  "scripts": {
    "dev:chrome": "wxt",
    "dev": "wxt -b firefox",
    "build:chrome": "wxt build",
    "build": "wxt build -b firefox",
    "zip:chrome": "wxt zip",
    "zip": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint-config-prettier": "^10.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.0.6",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "wxt": "^0.19.24"
  }
}