{
  "name": "environment-switch",
  "description": "Quickly switch project pages between environments",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev:chrome": "wxt",
    "dev": "wxt -b firefox",
    "build:chrome": "wxt build",
    "build": "wxt build -b firefox",
    "zip:chrome": "wxt zip",
    "zip": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "wxt": "^0.18.9"
  }
}